#<? cfg paf ?>
#
# 
inputKeys: {
    exposure: "backgroundSubtractedExposure"
    sourceSet: "sources"
}
outputKeys: {
    psf: "psf"
    cellSet: "cells"
    sdqa: "sdqa"
}

parameters: {
    psfDeterminationPolicy: {
        fluxLim: 3000.0
        clumpNSigma: 3.0
        #nonLinearSpatialFit: true
        nonLinearSpatialFit: false
        nEigenComponents: 4
        spatialOrder: 2
        nStarPerCell: 3
        kernelSize: 21
        borderWidth: 5
        nStarPerCellSpatialFit: 3
        constantWeight: false
        nIterForPsf: 3
        tolerance: 0.1
        reducedChi2ForPsfCandidates: 20.0
        sizeCellX: 512
        sizeCellY: 512
    }
}

