#<? cfg paf ?>
#
# 
inputKeys: {
    exposure: "backgroundSubtractedExposure"
    sourceSet: "sources"
}
outputKeys: {
    psf: "psf"
    cellSet: "cells"
    sdqa: "sdqa"
}

parameters: {
    psfDeterminationPolicy: {
        fluxLim: 3000.0
        clumpNSigma: 3.0
        nEigenComponents: 4
        spatialOrder: 2
        nStarPerCell: 3
        kernelSize: 25
        nStarPerCellSpatialFit: 3
        constantWeight: false
        nIterForPsf: 5
        tolerance: 0.1
        reducedChi2ForPsfCandidates: 10.0
        sizeCellX: 512 # 256
        sizeCellY: 512 # 256
    }
}

