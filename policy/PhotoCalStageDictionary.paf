#<?cfg paf dictionary ?>
#
# Dictionary for Photometric Zeropoint Stage.
# This file is currently empty - there are no parameters for this 
# stage. We include a policy file for consistency (and in case 
# we need to add parameters at a later date
#





definitions: {
     inputExposureKey: {
        type: "string"
        description: "specify clipboard key of input Exposure"
        minOccurs: 1
        maxOccurs: 1
        default: visitExposure
    }
     sourceMatchSetKey: {
        type: "string"
        description: "specify clipboard key of input SourceMatchSet"
        minOccurs: 1
        maxOccurs: 1
        default: SourceMatchSet
    }
    outputValueKey: {
        type: "string"
        description: "specify clipboard key of output PhotometricMagnitude Object"
        minOccurs: 1
        maxOccurs: 1
        default: photometricMagnitudeObject
    }
}
