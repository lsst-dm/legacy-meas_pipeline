                }
                outputKey: {
                    type: "string"
                    description: "specfy the clipboard key for the output 
                    		      DiaSourceSet"
                    minOccurs: 1                    
                    maxOccurs: 1
                }
            }
        }
        minOccurs: 1
    }
    ampBBoxKey: {
        type: "string"
        description: "Specify the amplifier's BBox"
        minOccurs: 1
        maxOccurs: 1
    }
    ccdWcsKey: {
        type: "string"
        description: "Specify the amplifier's Wcs Key"
        minOccurs: 1
        maxOccurs: 1
    }
