#<?cfg paf dictionary ?>
target: lsst.meas.pipeline.ComputeSourceSkyCoordsStage

definitions: {
    # input clipboard keys
    inputKeys: {
        type: "policy"
        minOccurs: 1 
        dictionary: {
            definitions: {
                sources: {
                    type: "string"
                    description: "clipboard key of the input SourceSet"
                    maxOccurs: 1
                    default: "sources"
                }
                wcs: {
                    type: "string"
                    description: "clipboard key of the WCS used to map pixel coordinates to sky coordinates"
                    maxOccurs: 1
                    default: "wcs"
                }
                exposure: {
                    type: "string"
                    description: "clipboard key of the exposure to obtain a WCS from if
                                  the clipboard does not directly reference a WCS.
                                 "
                    maxOccurs: 1
                    default: "exposure"
                }
            }
        }
    }
}
