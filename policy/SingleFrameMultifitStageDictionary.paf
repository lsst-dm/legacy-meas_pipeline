    #input clipboard keys
    inputKeys: {
        type: "policy"
        minOccurs: 1
        maxOccurs: 1
        dictionary: {
            definitions: {
                exposure: {
                    type: "string"
                    description: "clipboard key of the input exposure"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "exposure"
                }
                psf: {
                    type: "string"
                    description: "clipboard key of the input psf"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "psf"
                }
                fpSet: {
                    type: "string"
                    description: "clipboard key of the input footprint set"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "footprintSet"
                }
                sourceSet: {
                    type: "string"
                    description: "clipboard key of the input sources"
                    minOccurs: 1
                    maxOccurs: 1
                    default: "sourceSet"
                }
            }
        }
    }
    fitPolicy: {
        type: "policy"
        dictionary: @@meas_utils:policy/FitSourceModelsDictionary.paf
