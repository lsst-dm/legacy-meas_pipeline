#<? cfg paf dictionary ?>
#
# dictionary for SourceToDiaSourcestage

target: lsst.meas.pipeline.SourceToDiaSourceStage

definitions:
    data: {
        type: "policy"
        description: "Array of (input, output) key pairs. For every SourceSet  found in
                      the clipboard with key inputKey, the stage will output a DiaSourceSet
                      to the clipboard with key specified by outputKey"
        dictionary: {
            definitions: {
                inputKey: {
                    type: "string"
                    description: "specify the clipboard location of incoming SourceSet"
                    minOccurs: 0
                    maxOccurs: 1
                    default: "sourceSet"
                }
                outputKey: {
                    type: "string"
                    description: "specfy the clipboard key for the output DiaSourceSet"
                    minOccurs: 0
                    maxOccurs: 1
                    default: "diaSourceSet"
                }
            }
        }
        minOccurs: 1
    }
